GCH = $(PCH_PLACEHOLDER).gch
INCLUDES += -I../dependencies/include -Isrc
FORCE_INCLUDE +=
ALL_CPPFLAGS += $(CPPFLAGS) -MD -MP $(DEFINES) $(INCLUDES)
ALL_RESFLAGS += $(RESFLAGS) $(DEFINES) $(INCLUDES)
LIBS += -lGL -lglfw
LDDEPS +=
LINKCMD = $(CXX) -o "$@" $(OBJECTS) $(RESOURCES) $(ALL_LDFLAGS) $(LIBS)
define PREBUILDCMDS
endef
define PRELINKCMDS
endef

ifeq ($(config),debug)
TARGETDIR = ../bin/Debug-linux-x86_64/Core
TARGET = $(TARGETDIR)/libCore.so
OBJDIR = ../bin-obj/Debug-linux-x86_64/Core
DEFINES += -DBLOCKS_PLATFORM_LINUX -DBLOCKS_API_EXPORTS -DBLOCKS_DEBUG
ALL_CFLAGS += $(CFLAGS) $(ALL_CPPFLAGS) -m64 -fPIC -g
ALL_CXXFLAGS += $(CXXFLAGS) $(ALL_CPPFLAGS) -m64 -fPIC -g -std=c++20
ALL_LDFLAGS += $(LDFLAGS) -L../dependencies/lib -L/usr/lib64 -m64 -shared -Wl,-soname=libCore.so
define POSTBUILDCMDS
	@echo Running postbuild commands
	mkdir -p ../bin/Debug-linux-x86_64/Application
	cp -f ../bin/Debug-linux-x86_64/Core/libCore.so ../bin/Debug-linux-x86_64/Application/libCore.so
endef

